{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "layouts/*": ["src/layouts/*"],
      "pages/*": ["src/pages/*"],
      "components/*": ["src/components/*"],
      "asset/*": ["src/asset/*"],
    },
    "noEmit": true
  },
  "include": ["vite.config.ts"]
}
